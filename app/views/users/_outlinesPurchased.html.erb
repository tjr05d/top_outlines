<h4>Outlines Purchased</h4>
<div class="row">
  <% @user.purchases.each do |purchase| %>
  <div class="col s12 m4">
    <div class="card small">
      <div class="card-image">
        <img src='<%= purchase.outline.attachment.url(:thumb)%>' >
        <span class="card-title #00695c teal-text text-darken-3"> <%=purchase.outline.title%> </span>
      </div>
      <div class="card-content">
        <p><b>Course </b><%= purchase.outline.course.name %></p>
        <p><b>Sales </b><%= purchase.outline.purchases.count %></p>
      </div>
      <div class="card-action">
        <%= link_to "#{purchase.outline.title}", purchase.outline.attachment.url %>
      </div>
    </div>
  </div>

  <% end %>
</div>
