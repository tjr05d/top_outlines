<table class="table table-bordered table-striped">
  <h3><%= "#{current_user.school.name} Outlines" %></h3>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Owner</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_outlines_search.each do |outline| %>
    <tr>
      <td>
        <h4>Title:<%= outline.title %> </h4>
        <h5>Course:<%=outline.course.name %></h5>
        <h6>Professor:<%= outline.professor %></h6>
        <div><%= image_tag outline.attachment.url(:thumb) %></div>
      </td>
      <td><%= outline.description %></td>
      <td>Created by:<%= outline.seller %></td>
      <td>$<%= outline.price %>.00</td>
      <td><%= link_to 'Purchase Outline', outline, class: "btn btn-default"%></td>
    <% end %>
  </tbody>
</table>
