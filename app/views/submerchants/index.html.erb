<p id="notice"><%= notice %></p>

<h1>Listing Submerchants</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submerchants.each do |submerchant| %>
      <tr>
        <td><%= link_to 'Show', submerchant %></td>
        <td><%= link_to 'Edit', edit_submerchant_path(submerchant) %></td>
        <td><%= link_to 'Destroy', submerchant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submerchant', new_submerchant_path %>
